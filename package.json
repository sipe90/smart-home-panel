{
  "name": "tradfri-control-panel",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_PATH=src/server ts-node src/server/app.ts",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --host 0.0.0.0",
    "server": "cross-env NODE_PATH=src/server nodemon --host 0.0.0.0",
    "mock-api": "node mock/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-mock": "concurrently \"npm run mock-api\" \"npm run client\"",
    "test:client": "jest",
    "tradfri-auth": "cross-env NODE_PATH=src/server ./bin/tradfri.js authenticate",
    "tradfri-discover": "cross-env NODE_PATH=src/server ./bin/tradfri.js discover"
  },
  "dependencies": {
    "antd": "^3.9.2",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.4",
    "mdi-react": "^4.3.0",
    "morgan": "^1.9.1",
    "node-tradfri-client": "^1.5.0",
    "normalizr": "^3.2.4",
    "ramda": "^0.26.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-sticky": "^6.0.3",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/morgan": "^1.7.35",
    "@types/ramda": "^0.25.44",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-sticky": "^6.0.2",
    "@types/redux-form": "^7.4.13",
    "@types/redux-logger": "^3.0.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
